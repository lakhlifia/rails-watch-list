<h1>All Lists</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @lists.each do |list| %>
      <tr>
        <td><%= list.name %></td>
        <td>
          <%= link_to 'Show', list_path(list), class: 'btn btn-primary' %>
          <%# <%= link_to 'Add Bookmark', new_list_bookmark_path(list), class: 'btn btn-success' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New List', new_list_path, class: 'btn btn-success' %>
